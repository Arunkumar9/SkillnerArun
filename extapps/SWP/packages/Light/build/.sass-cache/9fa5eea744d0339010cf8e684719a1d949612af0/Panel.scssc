        @include border-management(
            $parent-cls: panel-#{$ui-label},
            $border-width: $ui-wrap-border-width,
            $border-color: $ui-wrap-border-color
        );
    }
}

.#{$prefix}panel-ghost {
    @include opacity($panel-ghost-opacity);
}

